datasource db {
    provider = "sqlite"
    url      = "file:dev.db"
}

generator client {
  provider = "cargo prisma"
  output   = "../src/prisma.rs"
}

generator custom_generator {
  provider    = "cargo custom-generator"
  output      = "../src/db_custom_generator.rs"
  client_path = "prisma"
}

model Account {
    id          String  @id
    displayName String
}